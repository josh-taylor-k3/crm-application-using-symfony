{
    "name": "oro/crm",
    "description": "The OroCRM distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "oro/bap": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laboro/bap"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/AddressBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/ConfigBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/DataFlowBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/FilterBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/FlexibleEntityBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/GridBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/MeasureBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/NavigationBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/SearchBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/SegmentationTreeBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/SoapBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/UIBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/UserBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/DataAuditBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/WindowsBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/JsFormValidationBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/SonataAdminBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/FilterBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/orocrm/AccountBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/orocrm/ContactBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laboro/TestFrameworkBundle.git"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "php app/console fos:js-routing:dump --target=web/js/routes.js"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "php app/console fos:js-routing:dump --target=web/js/routes.js"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
